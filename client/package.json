  "author": "Nils Hartmann",
  "license": "ISC",
  "dependencies": {
    "@types/moment": "^2.13.0",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "moment": "^2.15.1",
    "react": "^18.3.1",
    "react-datepicker": "7.4.0",
    "react-dom": "^18.3.1",
    "react-router": "6.26.2",
    "redbox-react": "1.6.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@babel/preset-react": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "babel-core": "^6.4.0",
    "babel-loader": "^9.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chalk": "^1.1.3",
    "cross-env": "^1.0.7",
    "css-loader": "^6.10.0",
    "enzyme": "^2.5.1",
    "eslint": "^9.12.0",
    "eslint-webpack-plugin": "^4.2.0",
    "file-loader": "^6.2.0",
    "jest": "29.0.0",
    "jest-fetch-mock": "^1.0.6",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.0",
    "rimraf": "^2.5.0",
    "style-loader": "^0.13.0",
    "ts-jest": "29.2.5",
    "ts-loader": "^9.5.1",
    "tslint-loader": "^3.5.4",
    "typescript": "^5.6.2",
    "url-loader": "^0.5.7",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "5.1.0"
  },
  "jest": {
    "scriptPreprocessor": "./node_modules/ts-jest/preprocessor.js",
    "testRegex": "(/__tests__/.*\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
